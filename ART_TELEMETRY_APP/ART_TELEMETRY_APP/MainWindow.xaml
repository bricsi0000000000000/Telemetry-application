<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="ART_TELEMETRY_APP.MainWindow"
        mc:Ignorable="d"
        Title="ART telemetry v_2.0"
        Height="700"
        Width="1300"
        WindowStartupLocation="CenterScreen"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        Background="{DynamicResource MaterialDesignPaper}"
        xmlns:dragablz="clr-namespace:Dragablz;assembly=Dragablz"
        xmlns:wpf="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        FontFamily="{materialDesign:MaterialDesignFont}" WindowStyle="ThreeDBorderWindow">
    
    <Grid>
        <dragablz:TabablzControl x:Name="MainMenuTabControl"/>
        <materialDesign:Snackbar MessageQueue="{materialDesign:MessageQueue}"
                                 x:Name="ErrorSnackbar"
                                 Foreground="White"
                                 />
        <!--<wpf:CartesianChart Name="chart"/>-->
        <!--<Canvas x:Name="canvas"/>-->
    </Grid>
</Window>
